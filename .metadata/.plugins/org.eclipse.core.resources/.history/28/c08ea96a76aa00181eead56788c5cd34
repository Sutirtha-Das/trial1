	}
	public static void printArray(int arr[]){
		for(int i=0;i<arr.length;i++){
			System.out.print(arr[i]);
		}
	}
	public static void leftRotate(int arr[],int d, int n){
		
	}
	public static void leftRotateByOne(int arr[]){
		int i;
		int temp = arr[0];
		for(i=0;i<arr.length;i++){
			arr[i] = arr[i+1];
		}
		arr[i]=temp;
	}
}
