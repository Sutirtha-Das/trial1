package corejava;

import java.lang.reflect.Method;

public class RunClass {

	
	public static void main(String[] args){
		Student s = new Student();
		Class c = s.getClass();
		System.out.println(c.getName());
		Method m[] = c.getDeclaredMethods();
		for(Method m1: m){
			System.out.println(m1.getName());
		}
		
		
	}
}
